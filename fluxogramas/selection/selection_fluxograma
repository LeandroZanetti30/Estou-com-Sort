digraph SelectionSort {
	rankdir=TB
	A [label="Início"]
	B [label="Para i de 0 até n-2"]
	C [label="min_idx = i"]
	D [label="Para j de i+1 até n-1"]
	E [label="Se A[j] < A[min_idx]"]
	F [label="min_idx = j"]
	G [label="Trocar A[i] com A[min_idx]"]
	H [label=Fim]
	A -> B
	B -> C
	C -> D
	D -> E
	E -> F [label=Sim]
	F -> D
	E -> D [label="Não" style=dashed]
	D -> G
	G -> B
	B -> H
}
